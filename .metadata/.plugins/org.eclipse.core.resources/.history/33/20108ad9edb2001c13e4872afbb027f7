package entrada_saida;

import java.util.Scanner;

public class Ex_4 {

	public static void main(String[] args) {
		
		Scanner ler = new Scanner(System.in);
		
		int a,b,c,d;
		double soma;
		
		a = ler.nextInt();
		b = ler.nextInt();
		c = ler.nextInt();
		d = ler.nextInt();
		
		soma = (a+b+c+d)/4;
		
		System.out.printf("A média artitmética é: " + soma);
			
	}

}
